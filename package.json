{
  "name": "air-quality-checker",
  "version": "0.0.1",
  "description": "A site to check air quality stations data in a friendly way",
  "main": "index.js",
  "scripts": {
    "mock-server": "json-server --routes ./src/mock-server/routes.json --watch ./src/mock-server/data/db.json",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch webpack.*.js --exec \"webpack-dev-server\" --mode=development",
    "start:prod": "nodemon --watch webpack.*.js --exec \"webpack-dev-server\" --mode=production",
    "build": "npm run build:dev",
    "build:dev": "rimraf dist && webpack --config webpack.dev.js",
    "build:prod": "rimraf dist && webpack -p --config webpack.prod.js",
    "test": "jest -c ./config/test/jest.json --verbose",
    "test:watch": "jest -c ./config/test/jest.json --verbose --watchAll -i"
  },
  "author": "Óscar Garcia Masiá",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^4.6.0",
    "json-server": "^0.14.0",
    "nodemon": "^1.18.8",
    "ts-jest": "^23.10.5",
    "ts-lint": "^4.5.1",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1"
  }
}
