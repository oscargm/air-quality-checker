{
	"name": "air-quality-checker",
	"version": "0.0.1",
	"description": "A site to check air quality stations data in a friendly way",
	"main": "index.js",
	"scripts": {
		"start": "npm run start:dev",
		"start:dev": "nodemon --watch webpack.*.js --exec webpack-dev-server --mode=development",
		"start:prod": "nodemon --watch webpack.*.js --exec \"webpack-dev-server\" --mode=production",
		"build": "npm run build:dev",
		"build:dev": "rimraf dist && webpack --config webpack.dev.js",
		"build:prod": "rimraf dist && webpack -p --config webpack.prod.js",
		"test": "jest -c ./config/test/jest.json --verbose",
		"test:watch": "jest -c ./config/test/jest.json --verbose --watchAll -i"
	},
	"author": "Óscar Garcia Masiá",
	"contributors": [
		"Josep Bergay Ribas"
	],
	"license": "ISC",
	"devDependencies": {
		"@types/react": "^16.8.20",
		"@types/react-dom": "^16.8.4",
		"@types/react-router-dom": "^4.3.4",
		"awesome-typescript-loader": "^5.2.1",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.0.6",
		"babel-polyfill": "^6.26.0",
		"concurrently": "^4.1.0",
		"copy-webpack-plugin": "^4.6.0",
		"json-server": "^0.14.2",
		"nodemon": "^1.19.1",
		"redux-devtools-extension": "^2.13.8",
		"redux-thunk": "^2.3.0",
		"ts-jest": "^23.10.5",
		"ts-lint": "^4.5.1",
		"typescript": "^3.5.2",
		"webpack": "^4.34.0",
		"webpack-cli": "^3.3.4",
		"webpack-dev-server": "^3.7.1"
	},
	"dependencies": {
		"@material-ui/core": "^3.9.3",
		"@material-ui/icons": "^3.0.2",
		"axios": "^0.19.0",
		"chart.js": "^2.8.0",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.5.0",
		"react": "^16.8.6",
		"react-chartkick": "^0.3.1",
		"react-dom": "^16.8.6",
		"react-redux": "^7.1.0",
		"react-router-dom": "^4.3.1",
		"redux": "^4.0.1",
		"redux-logger": "^3.0.6",
		"redux-saga": "^1.0.3",
		"reselect": "^4.0.0"
	}
}
